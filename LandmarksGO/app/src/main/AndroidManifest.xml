<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mosis.landmarksgo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".authentication.SignupActivity" />
        <activity android:name=".authentication.LoginActivity" />
        <activity android:name=".authentication.ResetPasswordActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".AddLandmark" />
        <activity android:name=".HighScore" />
        <activity android:name=".Friends" />
        <activity android:name=".FriendsAddNew" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>